<?xml version="1.0" encoding="UTF-8"?>
<labwc_config>
    <desktops number="5"/>
    <theme cornerRadius="0" dropShadows="yes"/>
    <libinput>
        <device category="non-touch" accelProfile="flat"/>
    </libinput>
    <keyboard numlock="off">
        <keybind key="W-S-q">
            <action name="Exit"/>
        </keybind>
        <keybind key="W-c">
            <action name="Close"/>
        </keybind>
        <keybind key="W-d">
            <action name="Iconify"/>
        </keybind>
        <keybind key="A-Tab">
            <action name="NextWindow"/>
        </keybind>
        <keybind key="W-S-r">
            <action name="Reconfigure"/>
        </keybind>
        <keybind key="W-Up">
            <action name="ToggleMaximize"/>
        </keybind>
        <keybind key="W-F11">
            <action name="ToggleFullscreen"/>
        </keybind>
        <keybind key="W-1">
            <action name="GoToDesktop" to="1"/>
        </keybind>
        <keybind key="W-2">
            <action name="GoToDesktop" to="2"/>
        </keybind>
        <keybind key="W-3">
            <action name="GoToDesktop" to="3"/>
        </keybind>
        <keybind key="W-4">
            <action name="GoToDesktop" to="4"/>
        </keybind>
        <keybind key="W-5">
            <action name="GoToDesktop" to="5"/>
        </keybind>
        <keybind key="W-S-1">
            <action name="SendToDesktop" to="1"/>
        </keybind>
        <keybind key="W-S-2">
            <action name="SendToDesktop" to="2"/>
        </keybind>
        <keybind key="W-S-3">
            <action name="SendToDesktop" to="3"/>
        </keybind>
        <keybind key="W-S-4">
            <action name="SendToDesktop" to="4"/>
        </keybind>
        <keybind key="W-S-5">
            <action name="SendToDesktop" to="5"/>
        </keybind>
        <keybind key="W-l">
            <action name="Execute" command="wofi"/>
        </keybind>
        <keybind key="W-Return">
            <action name="Execute" command="foot -m"/>
        </keybind>
        <keybind key="XF86_HomePage">
            <action name="Execute" command="firefox"/>
        </keybind>
        <keybind key="W-Left">
            <action name="SnapToEdge" direction="left"/>
        </keybind>
        <keybind key="W-Down">
            <action name="SnapToEdge" direction="down"/>
        </keybind>
        <keybind key="W-Right">
            <action name="SnapToEdge" direction="right"/>
        </keybind>
        <keybind key="XF86_AudioLowerVolume">
            <action name="Execute" command="amixer sset Master 5%-"/>
        </keybind>
        <keybind key="XF86_AudioRaiseVolume">
            <action name="Execute" command="amixer sset Master 5%+"/>
        </keybind>
        <keybind key="XF86_AudioMute">
            <action name="Execute" command="amixer sset Master toggle"/>
        </keybind>
    </keyboard>
</labwc_config>
